@import "../add_product.wxss";
.up-image{
  width: 120px;
  height: 120px;
  line-height: 120px;
  text-align: center;
  background: #f8f8f8;
  border-radius: 4px;
  margin: 10px 0;
  font-size: 13px;
  color: #aaa;
  position: relative;
  overflow: hidden;
}
.up-image-info{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

